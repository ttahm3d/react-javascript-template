{
	"name": "react-javascript-template",
	"version": "1.0.0",
	"description": "React starter template using webpack, babel, eslint, prettier, testing with jest and react-testing-library",
	"main": "index.js",
	"scripts": {
		"start": "webpack serve --config ./buildutils/webpack.dev.js --open",
		"build": "webpack --config ./buildutils/webpack.prod.js",
		"lint": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx,json}\"",
		"prettier": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
		"test": "jest"
	},
	"keywords": [
		"babel",
		"webpack",
		"starter",
		"react-starter"
	],
	"author": "Tahir Ahmed T",
	"license": "ISC",
	"dependencies": {
		"@testing-library/jest-dom": "^5.15.0",
		"@testing-library/react": "^12.1.2",
		"@testing-library/user-event": "^13.5.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	},
	"devDependencies": {
		"@babel/core": "^7.16.0",
		"@babel/plugin-transform-runtime": "^7.16.0",
		"@babel/preset-env": "^7.16.0",
		"@babel/preset-react": "^7.16.0",
		"babel-loader": "^8.2.3",
		"css-loader": "^6.5.1",
		"eslint": "^8.2.0",
		"eslint-config-airbnb": "^19.0.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-plugin-import": "^2.25.3",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-react": "^7.27.0",
		"eslint-plugin-react-hooks": "^4.3.0",
		"html-webpack-plugin": "^5.5.0",
		"jest": "^27.3.1",
		"prettier": "2.4.1",
		"sass": "^1.43.4",
		"sass-loader": "^12.3.0",
		"style-loader": "^3.3.1",
		"webpack": "^5.64.0",
		"webpack-cli": "^4.9.1",
		"webpack-dev-server": "^4.5.0",
		"webpack-merge": "^5.8.0"
	}
}
